require faiss

statement ok
CALL faiss_create('flat8', 8, 'IDMap,Flat');

statement ok
CREATE TABLE training AS SELECT * FROM 'test/sql/training.csv';

statement ok
CREATE TABLE queries AS SELECT * FROM 'test/sql/training.csv';

statement ok
